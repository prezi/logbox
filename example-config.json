{
    "categories": [
        {
            "input_glob": "first_category_of_logs/log-${date_glob}*",
            "name": "first_category",
            "rules": [
                {
                    "description": "Simply sorts logs of first category and puts it into place",
                    "match": "^(?<date>201[0-9]\\-[01][0-9]\\-[0-3][0-9]) (?<time_hour>[0-9]{2}):(?<time_min>[0-9]{2})(?:\\S+) (?<other_info>((\\S+\\s){6}))(?<source>\\w+) (?<rest>.*)$",
                    "name": "rulename",
                    "output_format": "${date} ${time_hour}:${time_min} ${other_info}${source} ${rest}",
                    "output_location": "first_category_dirname/${date}/${input_basename}",
                    "started_at": "2013-01-01"
                }
            ]
        },
        {
            "input_glob": "second_category_of_logs/log-${date_glob}*",
            "name": "second_category",
            "rules": [
                {
                    "description": "Simply sorts logs of type 2 and puts it into place",
                    "match": "^(?<date>201[0-9]\\-[01][0-9]\\-[0-3][0-9]) (?<time_hour>[0-9]{2}):(?<time_min>[0-9]{2})(?<rest>.*)$",
                    "name": "rule_1",
                    "output_format": "${date} ${time_hour}:${time_min}${rest}",
                    "output_location": "rule_1_dirname/${date}/${input_basename}",
                    "started_at": "2010-03-31"
                },
                {
                    "description": "Extract prezi reuses from kpi log",
                    "match": "^(?<date>201[0-9]\\-[01][0-9]\\-[0-3][0-9]) (?<time>[0-9]{2}:[0-9]{2}:.{6}) (?<hostname>[a-zA-Z0-9\\-\\._]+) kpi\\.event 4 (?<user_id>[0-9]+) (?<item_1>[0-9]+)( *)(?<item_2>[a-zA-Z0-9\\-_]*)$",
                    "name": "rule_2",
                    "output_format": "${date} ${time} ${user_id} ${item_1} ${item_2}",
                    "output_location": "rule_2_dirname/${date}/${input_basename}",
                    "started_at": "2010-03-10"
                }
            ]
        }

    ],
    "input_compression": "lzo",
    "input_location_prefix": "s3://my_logs_are_here/",
    "output_compression": "lzo",
    "output_location": "s3://my_logs_are_here/sorted_log/"
}